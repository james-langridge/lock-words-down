{
  "name": "syllable-game",
  "version": "1.0.0",
  "engines": {
    "node": "15.x"
  },
  "description": "An educational word game app for use by specialist teachers of dyslexic children.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "DEBUG=express:* DEBUG_COLORS=true DEBUG_FD=1 nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/james-langridge/syllable-game.git"
  },
  "keywords": [],
  "author": "James Langridge",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.848.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
